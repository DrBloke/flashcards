---
import "../styles/global.css";

import Flashcard from "../components/Flashcard.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
const pageTitle = "Greetings";

const cards = [
    { side1: "Buon giorno", side2: "Hello" },
    { side1: "Grazie", side2: "Thank you" },
];

const tags = ["Italian", "greetings"];
---

<BaseLayout pageTitle={pageTitle}>
    <Flashcard pageTitle={pageTitle} cards={cards} />
</BaseLayout>
