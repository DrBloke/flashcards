{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "node_modules/@awesome.me/webawesome/dist/custom-elements-jsx.d.ts",
    "**/*"
  ],
  "exclude": ["dist"],
  "compilerOptions": {
    "target": "ESNext",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}
